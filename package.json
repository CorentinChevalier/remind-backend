{
	"name": "remind-backend",
	"version": "1.0.0",
	"description": "api for re:mind app",
	"main": "app.js",
	"scripts": {
		"dev": "nodemon ./src/app.ts",
		"build": "tsc",
		"serve": "node ./dist/app.js",
		"migration:generate": "npx typeorm-ts-node-commonjs migration:generate $1 -d ./src/app-data-source.ts",
		"migrate": "npx typeorm-ts-node-commonjs migration:run -d ./src/app-data-source.ts",
		"migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/app-data-source.ts",
		"database:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/app-data-source.ts",
		"docker-compose": "docker compose -f docker-compose.yml up -d",
		"test": "jest --coverage"
	},
	"author": "Corentin Chevalier",
	"license": "ISC",
	"dependencies": {
		"@types/jsonwebtoken": "^9.0.5",
		"argon2": "^0.31.2",
		"express": "^4.18.2",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.11.3",
		"reflect-metadata": "^0.2.1",
		"typeorm": "^0.3.20"
	},
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@babel/preset-typescript": "^7.23.3",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.16",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"babel-jest": "^29.7.0",
		"dotenv": "^16.4.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"jest": "^29.7.0",
		"nodemon": "^3.0.3",
		"prettier": "3.2.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
